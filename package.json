{
  "name": "ethr-status-registry",
  "version": "1.1.1",
  "description": "Verifiable Credential status resolver using an ethereum contract as registry",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test-with-coverage": "jest --coverage && codecov",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "release": "semantic-release --debug"
  },
  "repository": "git@github.com:uport-project/ethr-status-registry.git",
  "keywords": [
    "Verifiable Credential",
    "W3C",
    "ethereum",
    "registry",
    "status check",
    "revocation",
    "jwt"
  ],
  "author": "Mircea Nistor <mircea.nistor@consensys.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/uport-project/ethr-status-registry/issues"
  },
  "homepage": "https://github.com/uport-project/ethr-status-registry#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.37",
    "codecov": "^3.6.1",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "prettier": "^2.0.5",
    "semantic-release": "^15.14.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "buffer": "^5.6.0",
    "credential-status": "^1.1.1",
    "did-jwt": "^3.0.0",
    "did-resolver": "^1.1.0",
    "ethjs-contract": "^0.2.3",
    "ethjs-provider-http": "^0.1.6",
    "ethjs-query": "^0.3.8",
    "js-sha3": "^0.8.0"
  }
}
